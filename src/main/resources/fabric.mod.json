{
  "schemaVersion": 1,
  "id": "visible_toggle_sprint",
  "version": "${version}",

  "name": "Visible Toggle Sprint",
  "description": "A simple mod to make toggle sprint & sneak easier!",
  "authors": [
    "Ender"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/visible-toggle-sprint",
    "issues": "https://github.com/theendercore/visible-toggle-sprint/issues"
  },

  "license": "CC BY-NC-SA 4.0",
  "icon": "assets/visible_toggle_sprint/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "com.theendercore.visibletogglesprint.VisibleToggleSprint"
    ],
    "modmenu": [
      "com.theendercore.visibletogglesprint.config.ModMenuCompat"
    ]
  },
  "mixins": [
    "visibletogglesprint.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "minecraft": ">=1.19.2",
    "java": ">=17",
    "yet-another-config-lib": ">=2.2.0"
  },
  "recommends": {
    "modmenu": ">=5.1.0-beta.4"
  }
